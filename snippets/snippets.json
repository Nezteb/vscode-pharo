{
	"nmethod": {
		"prefix": "nmethod",
		"body": "{ #category : #${1:accessing} }\n${2:${TM_FILENAME_BASE/(.*)\\..+$/$1/}} >> ${3:methodName} [\n\t$4\n]",
		"description": "New method in tonel file",
		"scope": "source.pharo"
	},
	"ncmethod": {
		"prefix": "ncmethod",
		"body": "{ #category : #${1:accessing} }\n${2:${TM_FILENAME_BASE/(.*)\\..+$/$1/}} class >> ${3:methodName} [\n\t$4\n]",
		"description": "New method in tonel file",
		"scope": "source.pharo"
	},
	"nclass": {
		"prefix": "nclass",
		"body": "Class {\n\t #name: #${1:${TM_FILENAME_BASE/(.*)\\..+$/$1/}},\n\t#superclass: #${2:Object},\n\t#instVars: [ $3],\n\t#category : #'${4:${TM_DIRECTORY/^.+[\\\\|\\\/](.*)$/$1/}}'\n}",
		"description": "New class in tonel file",
		"scope": "source.pharo"
	}
}